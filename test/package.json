{
  "name": "cypress-template",
  "version": "1.0.0",
  "description": "Cypress testing template with POM design pattern supporting E2E and API tests",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "test": "cypress run",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:api": "cypress run --spec 'cypress/e2e/api/**/*.cy.ts'",
    "test:chrome": "cypress run --browser chrome",
    "test:firefox": "cypress run --browser firefox",
    "test:edge": "cypress run --browser edge",
    "test:headed": "cypress run --headed --browser chrome",
    "test:spec": "cypress run --spec",
    "test:parallel": "cypress run --parallel --record --key",
    "test:dev": "cypress run --env environment=dev",
    "test:staging": "cypress run --env environment=staging",
    "test:prod": "cypress run --env environment=prod",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write '**/*.{ts,json,md}'",
    "format:check": "prettier --check '**/*.{ts,json,md}'",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t cypress-template .",
    "docker:run": "docker-compose up",
    "docker:run:detached": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose run --rm cypress-tests"
  },
  "keywords": [
    "cypress",
    "testing",
    "e2e",
    "api",
    "pom",
    "page-object-model",
    "typescript",
    "template"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cypress": "^15.6.0",
    "cypress-multi-reporters": "^2.0.5",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  }
}
